<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Log Suite Example" verbose="1">

<listeners>
		<listener class-name="testNgExamples.ListenerClass" /> 
		<listener class-name="testNgExamples.RetryListener"/>
		<listener class-name="testNgExamples.TestListener"/>
</listeners>

<test name="TestNG logs sample" preserve-order="true">
		<classes>
			<class name="testNgExamples.RetryFailedTestCases"> </class>
		</classes>
	</test>

</suite>

